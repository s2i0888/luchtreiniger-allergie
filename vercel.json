{
  "buildCommand": "hugo --gc --minify --cleanDestinationDir --forceSyncStatic",
  "outputDirectory": "public",
  "framework": "hugo",
  "installCommand": "npm install",
  "devCommand": "hugo server -D",
  "regions": ["ams1"],
  "github": {
    "silent": true
  },
  "env": {
    "HUGO_VERSION": "0.128.0",
    "HUGO_ENV": "production",
    "HUGO_ENABLEGITINFO": "true"
  },
  "redirects": [
    {
      "source": "/test-methodologie",
      "destination": "/test-methodologie/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}